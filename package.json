  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^3.7.0",
    "inquirer": "^1.2.1",
    "mocha": "^3.1.0",
    "nyc": "^8.3.0"
